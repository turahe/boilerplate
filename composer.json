{
    "name": "turahe/boilerplate",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "kalnoy/nestedset": "^6.0",
        "laravel-notification-channels/telegram": "^0.5.1",
        "laravel/cashier": "^13.2",
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-analytics": "^3.11",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-medialibrary": "^9.6",
        "spatie/laravel-menu": "^3.7",
        "spatie/laravel-permission": "^4.2",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "watson/sitemap": "^4.0",
        "ext-intl": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "knuckleswtf/scribe": "^3.1",
        "laravel/breeze": "^1.3",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "turahe/laravel-installer"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
